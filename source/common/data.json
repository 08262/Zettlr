    "filetypes": [
        ".txt",
        ".md",
        ".markdown"
    ],
    "ignoreDirs": [
        "^\\.",
        ".+?\\.app"
    ],
    "poll_time": 5000,
    "repo_url": "https://api.github.com/repos/Zettlr/Zettlr/",
